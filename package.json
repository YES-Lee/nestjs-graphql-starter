{
  "name": "nestjs-graphql-starter",
  "version": "0.0.1",
  "description": "nest web app 种子项目",
  "author": "Johnson",
  "license": "MIT",
  "keywords": [
    "nest",
    "nodejs",
    "graphql",
    "node框架"
  ],
  "repository": {
    "url": "https://github.com/YES-Lee/nestjs-graphql-starter.git",
    "type": "github"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=develop nest start --watch",
    "start:debug": "cross-env NODE_ENV=develop nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@nestjs/common": "^7.0.8",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.8",
    "@nestjs/graphql": "^7.3.1",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.8",
    "@types/passport-jwt": "^3.0.3",
    "apollo-server-express": "^2.12.0",
    "graphql": "^15.0.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^5.0.0",
    "lodash": "^4.17.19",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "nestjs-pino": "^1.1.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pino": "^6.2.0",
    "pino-pretty": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.8",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.0",
    "@types/supertest": "^2.0.8",
    "cross-env": "^7.0.2",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.0",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
